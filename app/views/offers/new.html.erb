<%= simple_form_for(@offer) do |f| %>
    <%= f.input :brand %>
    <%= f.input :model %>
    <%= f.input :fuel_mode %>
    <%= f.input :horsepower %>
    <%= f.input :color %>
    <%= f.input :price_per_day %>
    <%= f.input :min_age %>
    <%= f.input :photo %>
    <%= f.submit %>
<% end %>